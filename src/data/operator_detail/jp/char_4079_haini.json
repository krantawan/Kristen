      "voiceAsset": "char_4079_haini/CN_021",
      "voiceId": "CN_021",
      "placeType": "BATTLE_SELECT",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "選択時2",
      "voiceText": "何でも言ってちょうだい。ちゃんと聞いてるわ。",

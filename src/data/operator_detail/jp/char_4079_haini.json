      "unlockParam": []
    },
    {
      "voiceTitle": "タッチ1",
      "voiceText": "うっ……もう、久々に頭をぐりぐりしてほしいのかしら？",
      "voiceAsset": "char_4079_haini/CN_034",
      "voiceId": "CN_034",
      "placeType": "BUILDING_TOUCHING",

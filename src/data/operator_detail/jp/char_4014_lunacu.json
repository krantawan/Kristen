    },
    {
      "voiceTitle": "作戦中3",
      "voiceText": "あいつらをこっちに追い込んで！",
      "voiceAsset": "char_4014_lunacu/CN_027",
      "voiceId": "CN_027",
      "placeType": "BATTLE_SKILL_3",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",

    },
    {
      "voiceTitle": "作戦中4",
      "voiceText": "グズ駄獣、逃がさない！",
      "voiceAsset": "char_4014_lunacu/CN_028",
      "voiceId": "CN_028",
      "placeType": "BATTLE_SKILL_4",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",

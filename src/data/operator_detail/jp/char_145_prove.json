  "voice_lines": [
    {
      "voiceTitle": "秘書任命",
      "voiceText": "ドクター、部屋のお掃除をするときは呼んでね～。手伝ってあげるから。",
      "voiceAsset": "char_145_prove/CN_001",
      "voiceId": "CN_001",

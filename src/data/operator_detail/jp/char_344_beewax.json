    },
    {
      "voiceTitle": "作戦中2",
      "voiceText": "押しつぶせ。",
      "voiceAsset": "char_344_beewax/CN_026",
      "voiceId": "CN_026",
      "placeType": "BATTLE_SKILL_2",
      "voiceType": "ENUM",

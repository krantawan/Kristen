      "unlockParam": []
    },
    {
      "voiceTitle": "タッチ1",
      "voiceText": "うお！なになに、どした？",
      "voiceAsset": "char_4071_peper/CN_034",
      "voiceId": "CN_034",
      "placeType": "BUILDING_TOUCHING",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",

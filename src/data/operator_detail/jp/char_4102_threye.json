      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "配置1",
      "voiceText": "遠見の結果を皆に伝えてください。",
      "voiceAsset": "char_4102_threye/CN_023",
      "voiceId": "CN_023",
      "placeType": "BATTLE_PLACE",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []

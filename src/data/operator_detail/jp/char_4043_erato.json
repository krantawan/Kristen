    },
    {
      "voiceTitle": "戦闘開始",
      "voiceText": "私はみなさまの物語を見届けますわ。",
      "voiceAsset": "char_4043_erato/CN_020",
      "voiceId": "CN_020",
      "placeType": "BATTLE_FACE_ENEMY",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",

    {
      "voiceTitle": "編成",
      "voiceText": "私に任せてくれ、ドクター。",
      "voiceAsset": "char_1029_yato2/CN_017",
      "voiceId": "CN_017",
      "placeType": "SQUAD",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "隊長任命",
      "voiceText": "任されたからには、必ず作戦目標を達成してみせよう。",
      "voiceAsset": "char_1029_yato2/CN_018",
      "voiceId": "CN_018",
      "placeType": "SQUAD_FIRST",

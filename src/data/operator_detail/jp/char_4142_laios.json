    {
      "voiceTitle": "経験値上昇",
      "voiceText": "この魔物たち、本当に見れば見るほど面白いなぁ……",
      "voiceAsset": "char_4142_laios/CN_012",
      "voiceId": "CN_012",
      "placeType": "LEVEL_UP",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []

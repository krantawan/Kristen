      "voiceId": "CN_032",
      "placeType": "LOSE",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "基地配属",
      "voiceText": "軍の天幕で泊まったことも、畦道で寝たこともあります。こだわりはありませんよ。",
      "voiceAsset": "char_4121_zuole/CN_033",

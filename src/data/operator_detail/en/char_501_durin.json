  "voice_lines": [
    {
      "voiceTitle": "Appointed as Assistant",
      "voiceText": "Morning, Doctor... Mind if I lay out my sleeping bag here...? Thanks. The rest is up to you...",
      "voiceAsset": "char_501_durin/CN_001",
      "voiceId": "CN_001",
      "placeType": "HOME_PLACE",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "Talk 1",
      "voiceText": "How do I fight? Using Arts, of course... Want me to show you the power of earth?",
      "voiceAsset": "char_501_durin/CN_002",
      "voiceId": "CN_002",
      "placeType": "HOME_SHOW",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "Talk 2",
      "voiceText": "What time is it...? See, I don't work mornings, noons, afternoons, or nights...",
      "voiceAsset": "char_501_durin/CN_003",
      "voiceId": "CN_003",
      "placeType": "HOME_SHOW",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "Talk 3",
      "voiceText": "You'll make an S-sized uniform for me...? No thanks. My oversized one works as a sheet when I take naps... plus, it's really warm.",
      "voiceAsset": "char_501_durin/CN_004",
      "voiceId": "CN_004",
      "placeType": "HOME_SHOW",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "Talk after Trust Increase 1",
      "voiceText": "Dooooctoooooor? I need a break! I'm competing in the Speedy Sleep Contest.",
      "voiceAsset": "char_501_durin/CN_007",
      "voiceId": "CN_007",
      "placeType": "HOME_SHOW",
      "voiceType": "ONLY_TEXT",
      "unlockType": "FAVOR",
      "unlockParam": [
        {
          "valueStr": null,
          "valueInt": 40
        }
      ]
    },
    {
      "voiceTitle": "Talk after Trust Increase 2",
      "voiceText": "Doctor, check this out. This isn't my normal wand, it's a pendulum. You're feeling very sleepy and will nap in five minutes without giving me any work... Zzzz...",
      "voiceAsset": "char_501_durin/CN_008",
      "voiceId": "CN_008",
      "placeType": "HOME_SHOW",
      "voiceType": "ONLY_TEXT",
      "unlockType": "FAVOR",
      "unlockParam": [
        {
          "valueStr": null,
          "valueInt": 100
        }
      ]
    },
    {
      "voiceTitle": "Talk after Trust Increase 3",
      "voiceText": "I was hoping I could tell you about my homeland, Doctor.",
      "voiceAsset": "char_501_durin/CN_009",
      "voiceId": "CN_009",
      "placeType": "HOME_SHOW",
      "voiceType": "ONLY_TEXT",
      "unlockType": "FAVOR",
      "unlockParam": [
        {
          "valueStr": null,
          "valueInt": 200
        }
      ]
    },
    {
      "voiceTitle": "Idle",
      "voiceText": "After you went to sleep, I somehow didn't feel sleepy at all anymore... Hmmmph...",
      "voiceAsset": "char_501_durin/CN_010",
      "voiceId": "CN_010",
      "placeType": "HOME_WAIT",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "Onboard",
      "voiceText": "Hello? Hellooooo? Can you hear me? Down here. Down heeeeere! Nice meeting you, big Doctor. I'm Durin the Caster.",
      "voiceAsset": "char_501_durin/CN_011",
      "voiceId": "CN_011",
      "placeType": "GACHA",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "Watching Battle Record",
      "voiceText": "Whoa... I've fiiiinally gotten s-tron-ger.",
      "voiceAsset": "char_501_durin/CN_012",
      "voiceId": "CN_012",
      "placeType": "LEVEL_UP",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "Added to Squad",
      "voiceText": "The secret to staying healthy is to sleep whenever you can.",
      "voiceAsset": "char_501_durin/CN_017",
      "voiceId": "CN_017",
      "placeType": "SQUAD",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "Appointed as Squad Leader",
      "voiceText": "Captain's orders: everyone, save your energy and take a nap!",
      "voiceAsset": "char_501_durin/CN_018",
      "voiceId": "CN_018",
      "placeType": "SQUAD_FIRST",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "Depart",
      "voiceText": "I wanna dig a hole in the ground and make a shelter to sleep in, if you don't mind.",

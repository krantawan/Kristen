  "voice_lines": [
    {
      "voiceTitle": "Appointed as Assistant",
      "voiceText": "What's up, Doctor? Me, I'm just on standby.",
      "voiceAsset": "char_500_noirc/CN_001",
      "voiceId": "CN_001",
      "placeType": "HOME_PLACE",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "Talk 1",
      "voiceText": "My shield? Fun fact: I personally provide a portion of the energy it needs.",
      "voiceAsset": "char_500_noirc/CN_002",
      "voiceId": "CN_002",
      "placeType": "HOME_SHOW",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "Talk 2",
      "voiceText": "What I look like beneath the mask? Well, I... Actually, let's keep it secret for a bit longer.",
      "voiceAsset": "char_500_noirc/CN_003",
      "voiceId": "CN_003",
      "placeType": "HOME_SHOW",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "Talk 3",
      "voiceText": "Man, nothing better than a break after you finish a mission.",
      "voiceAsset": "char_500_noirc/CN_004",
      "voiceId": "CN_004",
      "placeType": "HOME_SHOW",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "Talk after Trust Increase 1",
      "voiceText": "Doctor, if I get to know you better, I figure my shield's gonna get even sturdier.",
      "voiceAsset": "char_500_noirc/CN_007",
      "voiceId": "CN_007",
      "placeType": "HOME_SHOW",
      "voiceType": "ONLY_TEXT",
      "unlockType": "FAVOR",
      "unlockParam": [
        {
          "valueStr": null,
          "valueInt": 40
        }
      ]
    },
    {
      "voiceTitle": "Talk after Trust Increase 2",
      "voiceText": "Course I know where my limit's at. But I don't want Yato to worry about me, which is what matters. We'll worry about the rest some other time!",
      "voiceAsset": "char_500_noirc/CN_008",
      "voiceId": "CN_008",
      "placeType": "HOME_SHOW",
      "voiceType": "ONLY_TEXT",
      "unlockType": "FAVOR",
      "unlockParam": [
        {
          "valueStr": null,
          "valueInt": 100
        }
      ]
    },
    {
      "voiceTitle": "Talk after Trust Increase 3",
      "voiceText": "There's always the need for guys like me, you know? Nah, I'm not trying to act high and mighty. What I'm saying is: when you have a VIP plus a lot of danger, you need a guy to stand between them, that's all.",
      "voiceAsset": "char_500_noirc/CN_009",
      "voiceId": "CN_009",
      "placeType": "HOME_SHOW",
      "voiceType": "ONLY_TEXT",
      "unlockType": "FAVOR",
      "unlockParam": [
        {
          "valueStr": null,
          "valueInt": 200
        }
      ]
    },
    {
      "voiceTitle": "Idle",
      "voiceText": "Sometimes you've just gotta take off the mask and catch a breather.",
      "voiceAsset": "char_500_noirc/CN_010",
      "voiceId": "CN_010",
      "placeType": "HOME_WAIT",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []

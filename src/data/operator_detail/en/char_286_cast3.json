  "voice_lines": [
    {
      "voiceTitle": "Appointed as Assistant",
      "voiceText": "Thank you for choosing Castle-3! I am honored to serve as your assistant, Doctor.",
      "voiceAsset": "char_286_cast3/CN_001",
      "voiceId": "CN_001",
      "placeType": "HOME_PLACE",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "Talk 1",
      "voiceText": "Doctor, please do not be intimidated by my appearance. My sturdy metal shell, modified by Master Closure herself, will make you invincible on the battlefield.",
      "voiceAsset": "char_286_cast3/CN_002",
      "voiceId": "CN_002",
      "placeType": "HOME_SHOW",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "Talk 2",
      "voiceText": "Warning: Do not strike this unit. It may cause hardware damage.",
      "voiceAsset": "char_286_cast3/CN_003",
      "voiceId": "CN_003",
      "placeType": "HOME_SHOW",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "Talk 3",
      "voiceText": "The topography in the wild may be treacherous, but Castle-3 is fitted with an engine with outstanding performance and a 4WD mode to whisk you away from danger.",
      "voiceAsset": "char_286_cast3/CN_004",
      "voiceId": "CN_004",
      "placeType": "HOME_SHOW",
      "voiceType": "ENUM",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "Talk after Trust Increase 1",
      "voiceText": "Doctor, if your life is ever in danger, just summon Castle-3! Castle-3 was created specifically for combat purposes. I am a top-class defensive warfighting robot.",
      "voiceAsset": "char_286_cast3/CN_007",
      "voiceId": "CN_007",
      "placeType": "HOME_SHOW",
      "voiceType": "ONLY_TEXT",
      "unlockType": "FAVOR",
      "unlockParam": [
        {
          "valueStr": null,
          "valueInt": 40
        }
      ]
    },
    {
      "voiceTitle": "Talk after Trust Increase 2",
      "voiceText": "Castle-3 is honored to be at your side. Doctor, should you require me to strike down your foes, I shall go through heaven and hell to see it done!",
      "voiceAsset": "char_286_cast3/CN_008",
      "voiceId": "CN_008",
      "placeType": "HOME_SHOW",
      "voiceType": "ONLY_TEXT",
      "unlockType": "FAVOR",
      "unlockParam": [
        {
          "valueStr": null,
          "valueInt": 100
        }
      ]
    },
    {
      "voiceTitle": "Talk after Trust Increase 3",
      "voiceText": "Castle-3's official manager is 'Master Closure,' but please understand that you are even more important to Castle-3 than Master is, in most cases. I am your devoted, trustworthy battle companion!",
      "voiceAsset": "char_286_cast3/CN_009",
      "voiceId": "CN_009",
      "placeType": "HOME_SHOW",
      "voiceType": "ONLY_TEXT",
      "unlockType": "FAVOR",
      "unlockParam": [
        {
          "valueStr": null,
          "valueInt": 200
        }
      ]
    },
    {

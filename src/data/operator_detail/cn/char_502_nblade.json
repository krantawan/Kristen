    },
    {
      "voiceTitle": "交谈1",
      "voiceText": "即使在工作之外的时间里，我也会注意您的状况。",
      "voiceAsset": "char_502_nblade/CN_002",
      "voiceId": "CN_002",
      "placeType": "HOME_SHOW",
      "voiceType": "ONLY_TEXT",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "交谈2",

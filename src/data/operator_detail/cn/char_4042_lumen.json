      "unlockParam": []
    },
    {
      "voiceTitle": "观看作战记录",
      "voiceText": "我们今后......必须得对付这么吓人的东西吗？",
      "voiceAsset": "char_4042_lumen/CN_012",
      "voiceId": "CN_012",
      "placeType": "LEVEL_UP",
      "voiceType": "ONLY_TEXT",
      "unlockType": "DIRECT",
      "unlockParam": []
    },

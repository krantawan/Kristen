    },
    {
      "voiceTitle": "作战中4",
      "voiceText": "不用担心，救援成功！",
      "voiceAsset": "char_466_qanik/CN_028",
      "voiceId": "CN_028",
      "placeType": "BATTLE_SKILL_4",

    {
      "voiceTitle": "作战中3",
      "voiceText": "正在规划最合适的救援方案。",
      "voiceAsset": "char_466_qanik/CN_027",
      "voiceId": "CN_027",
      "placeType": "BATTLE_SKILL_3",

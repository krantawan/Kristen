    },
    {
      "voiceTitle": "选中干员2",
      "voiceText": "我已经准备好了这里的地图。",
      "voiceAsset": "char_466_qanik/CN_022",
      "voiceId": "CN_022",
      "placeType": "BATTLE_SELECT",
      "voiceType": "ONLY_TEXT",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "部署1",
      "voiceText": "不用担心，救援队马上就到。",
      "voiceAsset": "char_466_qanik/CN_023",
      "voiceId": "CN_023",
      "placeType": "BATTLE_PLACE",
      "voiceType": "ONLY_TEXT",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "部署2",
      "voiceText": "在此之前请您保存体力，等待救援。",
      "voiceAsset": "char_466_qanik/CN_024",

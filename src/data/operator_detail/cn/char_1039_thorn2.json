      "placeType": "THREE_STAR",
      "voiceType": "ONLY_TEXT",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "非3星结束行动",
      "voiceText": "只是强弩之末，不必追击，我们返航吧。",
      "voiceAsset": "char_1039_thorn2/CN_031",
      "voiceId": "CN_031",
      "placeType": "TWO_STAR",

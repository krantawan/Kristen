      "placeType": "BATTLE_FACE_ENEMY",
      "voiceType": "ONLY_TEXT",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "选中干员1",
      "voiceText": "让我一个人待着，我知道该怎么做。",
      "voiceAsset": "char_1039_thorn2/CN_021",

    {
      "voiceTitle": "进驻设施",
      "voiceText": "我知道该去哪，我会自行处理。",
      "voiceAsset": "char_4132_ascln/CN_033",
      "voiceId": "CN_033",
      "placeType": "BUILDING_PLACE",
      "voiceType": "ONLY_TEXT",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "戳一下",
      "voiceText": "你再试试看。",

    },
    {
      "voiceTitle": "部署1",
      "voiceText": "那么，谁来做我的对手？",
      "voiceAsset": "char_131_flameb/CN_023",
      "voiceId": "CN_023",
      "placeType": "BATTLE_PLACE",
      "voiceType": "ONLY_TEXT",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {

      "unlockParam": []
    },
    {
      "voiceTitle": "完成高难行动",
      "voiceText": "这是我未曾料想过的，最好的结果，博士。",
      "voiceAsset": "char_4025_aprot2/CN_029",
      "voiceId": "CN_029",
      "placeType": "FOUR_STAR",

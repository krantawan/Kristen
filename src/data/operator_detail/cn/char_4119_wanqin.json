      "unlockParam": []
    },
    {
      "voiceTitle": "选中干员1",
      "voiceText": "到——不是......我在。",
      "voiceAsset": "char_4119_wanqin/CN_021",
      "voiceId": "CN_021",
      "placeType": "BATTLE_SELECT",
      "voiceType": "ONLY_TEXT",

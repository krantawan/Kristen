    },
    {
      "voiceTitle": "作战中3",
      "voiceText": "地形复杂，跟随我的脚步来。",
      "voiceAsset": "char_4119_wanqin/CN_027",
      "voiceId": "CN_027",
      "placeType": "BATTLE_SKILL_3",
      "voiceType": "ONLY_TEXT",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {

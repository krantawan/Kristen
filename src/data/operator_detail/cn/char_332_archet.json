      "placeType": "GACHA",
      "voiceType": "ONLY_TEXT",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "观看作战记录",
      "voiceText": "哦哦，这就是罗德岛干员们的战斗方式吗！有模有样的嘛！",
      "voiceAsset": "char_332_archet/CN_012",
      "voiceId": "CN_012",
      "placeType": "LEVEL_UP",

      "voiceId": "CN_022",
      "placeType": "BATTLE_SELECT",
      "voiceType": "ONLY_TEXT",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "部署1",
      "voiceText": "情况一目了然。",
      "voiceAsset": "char_4172_xingzh/CN_023",
      "voiceId": "CN_023",
      "placeType": "BATTLE_PLACE",
      "voiceType": "ONLY_TEXT",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "部署2",
      "voiceText": "要记的有很多。",

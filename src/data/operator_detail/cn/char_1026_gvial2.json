    },
    {
      "voiceTitle": "行动开始",
      "voiceText": "伙计们，让我们大闹一场吧！",
      "voiceAsset": "char_1026_gvial2/CN_020",
      "voiceId": "CN_020",
      "placeType": "BATTLE_FACE_ENEMY",
      "voiceType": "ONLY_TEXT",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "选中干员1",
      "voiceText": "博士，把大家伙留给我！",
      "voiceAsset": "char_1026_gvial2/CN_021",
      "voiceId": "CN_021",
      "placeType": "BATTLE_SELECT",
      "voiceType": "ONLY_TEXT",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {

      "placeType": "SQUAD",
      "voiceType": "ONLY_TEXT",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "任命队长",
      "voiceText": "不该再有更多生命消逝了。",
      "voiceAsset": "char_1012_skadi2/CN_018",
      "voiceId": "CN_018",
      "placeType": "SQUAD_FIRST",

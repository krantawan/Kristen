      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "作战中4",
      "voiceText": "别害怕，我的同胞们......很快就要到了。",
      "voiceAsset": "char_1012_skadi2/CN_028",
      "voiceId": "CN_028",
      "placeType": "BATTLE_SKILL_4",

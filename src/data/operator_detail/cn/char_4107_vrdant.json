    },
    {
      "voiceTitle": "选中干员2",
      "voiceText": "请赶快下令，敌人马上就到我面前了......",
      "voiceAsset": "char_4107_vrdant/CN_022",
      "voiceId": "CN_022",
      "placeType": "BATTLE_SELECT",

      "voiceId": "CN_033",
      "placeType": "BUILDING_PLACE",
      "voiceType": "ONLY_TEXT",
      "unlockType": "DIRECT",
      "unlockParam": []
    },
    {
      "voiceTitle": "戳一下",
      "voiceText": "别这么吓我！不要命了？",
      "voiceAsset": "char_4014_lunacu/CN_034",
      "voiceId": "CN_034",
      "placeType": "BUILDING_TOUCHING",

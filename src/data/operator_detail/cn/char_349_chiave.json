  "voice_lines": [
    {
      "voiceTitle": "任命助理",
      "voiceText": "喂，博士，有没有什么好玩的事情？",
      "voiceAsset": "char_349_chiave/CN_001",
      "voiceId": "CN_001",
      "placeType": "HOME_PLACE",
